{
  "name": "@founders-day/frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:turbopack": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run test/hooks test/fixtures test/database test/components",
    "test:integration": "vitest run test/api",
    "test:performance": "vitest run test/performance",
    "test:square": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:integration:enhanced": "node test-admin-integration-enhanced.js",
    "test:integration:vitest": "vitest run test/integration/",
    "test:mock-server": "node test/helpers/mock-api-server.js",
    "test:integration:local": "concurrently \"npm run test:mock-server\" \"wait-on http://localhost:3001 && npm run test:integration:enhanced\" --kill-others --success first",
    "test:all": "npm run test:square && npm run test:integration:enhanced",
    "monitor:setup": "node scripts/setup-monitoring-fixed.js",
    "monitor:test": "node scripts/test-monitoring.js",
    "monitor:validate": "npm run monitor:test && npm run build",
    "typecheck": "tsc --noEmit",
    "test:cucumber": "cucumber-js",
    "test:cucumber:watch": "cucumber-js --watch",
    "test:cucumber:smoke": "cucumber-js --profile smoke",
    "test:cucumber:parallel": "cucumber-js --profile parallel",
    "test:cucumber:sequential": "cucumber-js --profile sequential",
    "test:cucumber:ci": "cucumber-js --profile ci",
    "test:cucumber:fast": "cucumber-js --config cucumber.fast.js",
    "test:cucumber:fast:smoke": "cucumber-js --config cucumber.fast.js --profile smoke",
    "test:cucumber:benchmark": "cucumber-js --config cucumber.fast.js --profile benchmark",
    "test:cucumber:report": "cucumber-js --format html:reports/cucumber-report.html",
    "test:fast": "npm run test:cucumber:fast",
    "report:merge": "cucumber-merge-reports reports/*.json -o reports/merged-report.json",
    "report:generate": "cucumber-html-formatter -i reports/merged-report.json -o reports/final-report.html"
  },
  "dependencies": {
    "@axe-core/react": "^4.10.2",
    "@faker-js/faker": "^9.9.0",
    "@founders-day/shared-types": "file:packages/shared-types",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@square/web-sdk": "^2.1.0",
    "@stripe/stripe-js": "^7.5.0",
    "@tanstack/react-query": "^5.83.0",
    "axe-core": "^4.10.3",
    "bcryptjs": "^2.4.3",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jest-axe": "^10.0.0",
    "jose": "^5.2.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "picocolors": "^1.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.3",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.0",
    "workbox-webpack-plugin": "^7.3.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.0",
    "@sentry/nextjs": "^9.42.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/cucumber": "^7.0.3",
    "@types/newrelic": "^9.14.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "jsdom": "^25.0.1",
    "msw": "^2.6.8",
    "newrelic": "^13.0.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "vitest": "^2.1.8",
    "wait-on": "^8.0.4",
    "web-vitals": "^5.0.3",
    "@cucumber/cucumber": "^9.5.1",
    "@cucumber/pretty-formatter": "^2.0.1",
    "@supabase/supabase-js": "^2.52.1",
    "@types/chai": "^5.2.2",
    "axios": "^1.11.0",
    "chai": "^5.2.1",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "playwright": "^1.40.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}